<Page x:Class="FinalProject.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:System.Windows.Controls;assembly=PresentationFramework"
      mc:Ignorable="d" 
      d:DesignHeight="415" d:DesignWidth="785"
      Title="Page1"
      Background="White">

    <Grid>
        <Image HorizontalAlignment="Right" Height="415" Margin="0,0,0,0" VerticalAlignment="Top" Source="Images/diary.jpg"/>
        <TextBox Name="Diary" HorizontalAlignment="Right" Height="301" Margin="0,60,360,0" VerticalAlignment="Top" Width="190" Background="#00000000" TextWrapping="Wrap" AcceptsReturn="True" FontSize="11.3" CaretBrush="Black" BorderBrush="Black"/>
        <Button x:Name="Save" Content="Save" HorizontalAlignment="Right" Margin="0,365,420,0" VerticalAlignment="Top" Width="75" Click="save"/>
        <Button x:Name="Return" Content="Home" HorizontalAlignment="Left" Height="43" Margin="10,24,0,0" VerticalAlignment="Top" Width="124" Click="Return_Clicked"/>
        <Rectangle Fill="#7FFF2104" Height="43" Margin="515,60,0,0" Stroke="#000000" Width="190" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Rectangle Fill="#7FFF8000" Height="43" Margin="515,103,0,0" Stroke="#000000" Width="190" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Rectangle Fill="#7FE2FF00" Height="43" Margin="515,146,0,0" Stroke="#000000" Width="190" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Rectangle Fill="#7F3AFF00" Height="43" Margin="515,189,0,0" Stroke="#000000" Width="190" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Rectangle Fill="#7F00FFFF" Height="43" Margin="515,232,0,0" Stroke="#000000" Width="190" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Rectangle Fill="#7F002EFF" Height="43" Margin="515,275,0,0" Stroke="#000000" Width="190" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Rectangle Fill="#7F8500FF" Height="43" Margin="515,318,0,0" Stroke="#000000" Width="190" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <TextBlock x:Name="sub1" HorizontalAlignment="Left" Margin="515,60,0,0" TextWrapping="Wrap" Text="Subject 1 " VerticalAlignment="Top" Width="190" Height="43" FontSize="16" TextAlignment="Center" FontWeight="SemiBold"/>
        <TextBlock x:Name="sub2" HorizontalAlignment="Left" Margin="515,103,0,0" TextWrapping="Wrap" Text="Subject 2 " VerticalAlignment="Top" Width="190" Height="43" FontSize="16" TextAlignment="Center" FontWeight="SemiBold"/>
        <TextBlock x:Name="sub3" HorizontalAlignment="Left" Margin="515,146,0,0" TextWrapping="Wrap" Text="Subject 3 " VerticalAlignment="Top" Width="190" Height="43" FontSize="16" TextAlignment="Center" FontWeight="SemiBold"/>
        <TextBlock x:Name="sub4" HorizontalAlignment="Left" Margin="515,189,0,0" TextWrapping="Wrap" Text="Subject 4 " VerticalAlignment="Top" Width="190" Height="43" FontSize="16" TextAlignment="Center" FontWeight="SemiBold"/>
        <TextBlock x:Name="sub5" HorizontalAlignment="Left" Margin="515,232,0,0" TextWrapping="Wrap" Text="Subject 5 " VerticalAlignment="Top" Width="190" Height="43" FontSize="16" TextAlignment="Center" FontWeight="SemiBold"/>
        <TextBlock x:Name="sub6" HorizontalAlignment="Left" Margin="515,275,0,0" TextWrapping="Wrap" Text="Subject 6 " VerticalAlignment="Top" Width="190" Height="43" FontSize="16" TextAlignment="Center" FontWeight="SemiBold"/>
        <TextBlock x:Name="sub7" HorizontalAlignment="Left" Margin="515,318,0,0" TextWrapping="Wrap" Text="Subject 7 " VerticalAlignment="Top" Width="190" Height="43" FontSize="16" TextAlignment="Center" FontWeight="SemiBold"/>
        <TextBlock x:Name="date_top" HorizontalAlignment="Left" Margin="235,39,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <local:Frame Name="DiaryFrame" NavigationUIVisibility="Hidden" Navigated="DiaryFrame_Navigated" >
        </local:Frame>
        

    </Grid>
</Page>
