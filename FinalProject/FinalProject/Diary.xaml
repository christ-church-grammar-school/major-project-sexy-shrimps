<Page x:Class="FinalProject.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:System.Windows.Controls;assembly=PresentationFramework"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1"
      Background="#FF00305E">

    <Grid>
        <Rectangle Fill="Gold" HorizontalAlignment="Left" Height="450" Stroke="Black" VerticalAlignment="Top" Width="164" StrokeThickness="0"/>
        <TextBox Name="Diary" HorizontalAlignment="Left" Height="335" Margin="225,45,0,0" VerticalAlignment="Top" Width="507" RenderTransformOrigin="0.49,0.436"/>
        <Button Background="Gold" Content="Save" Foreground ="#FF00305E" HorizontalAlignment="Left" Margin="408,394,0,0" VerticalAlignment="Top" Width="148" Click="Save" Height="32" FontSize="18"/>
        <Button x:Name="Return" Background="#FF00305E" Foreground="Gold" Content="Home" HorizontalAlignment="Left" Height="43" Margin="10,24,0,0" VerticalAlignment="Top" Width="124" Click="Return_Clicked"/>
        <Rectangle Fill="#FF00305E" HorizontalAlignment="Left" Height="359" Margin="32,91,0,0" Stroke="Black" StrokeThickness="0" VerticalAlignment="Top" Width="188"/>
        <Rectangle Fill="Gold" HorizontalAlignment="Left" Height="41" Margin="614,409,0,0" Stroke="Black" StrokeThickness="0" VerticalAlignment="Top" Width="186"/>
        <Rectangle Fill="Gold" HorizontalAlignment="Left" Height="409" Margin="764,0,0,0" Stroke="Black" StrokeThickness="0" VerticalAlignment="Top" Width="36"/>
        <Label Content="Diary" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="71,122,0,0" Height="93" Width="161" Foreground="White" FontWeight="Bold" FontSize="48"/>
        <local:Frame Name="DiaryFrame" NavigationUIVisibility="Hidden" Navigated="DiaryFrame_Navigated" />
    </Grid>
</Page>
